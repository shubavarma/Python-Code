   "source": [
    "# Write a program to remove vowels from the entire alphabets\n",
    "x=input(\"Type your string here to remove vowels: \")\n",
    "string=\"\"\n",
    "for y in x:\n",
    "    if y==\"a\"or y==\"e\" or y==\"i\" or y==\"o\" or y==\"u\":\n",
    "        continue\n",
    "    else: \n",
    "        string=string+y\n",
    "print(string)    \n"
